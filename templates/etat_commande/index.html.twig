            <tr>
                <th>Id</th>
                <th>LibelleEtatCommande</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for etat_commande in etat_commandes %}
            <tr>
                <td>{{ etat_commande.id }}</td>
